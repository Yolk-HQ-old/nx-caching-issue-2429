{
  "version": 1,
  "projects": {
    "myapp": {
      "root": "apps/myapp",
      "sourceRoot": "apps/myapp",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/myapp",
            "outputPath": "dist/apps/myapp"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "myapp:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "myapp:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "myapp:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/myapp/.eslintrc",
            "tsConfig": [
              "apps/myapp/tsconfig.json",
              "apps/myapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/myapp/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/myapp/jest.config.js",
            "tsConfig": "apps/myapp/tsconfig.spec.json"
          }
        }
      }
    },
    "myapp-e2e": {
      "root": "apps/myapp-e2e",
      "sourceRoot": "apps/myapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/myapp-e2e/cypress.json",
            "tsConfig": "apps/myapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "myapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "myapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/myapp-e2e/.eslintrc",
            "tsConfig": ["apps/myapp-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/myapp-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "myapp"
}
